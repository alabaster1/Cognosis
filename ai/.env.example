# LLM Provider Configuration
# Options: "gemini" (default) or "openai"
LLM_PROVIDER=gemini

# Gemini API Configuration (if using Gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Configuration (if using OpenAI)
# OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/cognosis

# Service Configuration
PORT=8001
AI_SERVICE_HOST=0.0.0.0

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Agent Configuration (models are auto-selected based on LLM_PROVIDER)
MAX_TOKENS=1000
TEMPERATURE=0.7

# Logging
LOG_LEVEL=info
